import random

random_integer = random.randint(1, 10)
print(random_integer)

if random_integer <= 5:
    print("Heads")
elif random_integer >= 6:
    print("Tails")

names_string = input("Give me everybody's names, seperated by a comma.")
names = names_string.split(", ")

random_name = random.choice(names)
print(random_name)

