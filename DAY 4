eleccion = input("select one ")
computer = ("piedra","papel","tijera")
juego = random.choice(computer)
print(juego)

if eleccion == "papel":
    if juego == "piedra":
        print("you win!")
    elif juego == "papel":
        print("draw")
    elif juego == "tijera":
        print("you lose")

elif eleccion == "piedra":
    if juego == "piedra":
        print("draw")
    elif juego == "papel":
        print("you lose")
    elif juego == "tijera":
        print("you win!")

elif eleccion == "tijera":
    if juego == "piedra":
        print("you lose")
    elif juego == "papel":
        print("you win!")
    elif juego == "tijera":
        print("draw")
